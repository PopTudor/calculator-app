<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="android:Theme.Material.NoActionBar.Fullscreen">
        <!-- Customize your theme here. -->
    </style>
    <style name="leftSymbols" parent="android:Widget.Material.Button.Borderless" >
        <item name="android:textColor">#fff</item>
        <item name="android:background">#888</item>
        <item name="android:textSize">@dimen/fontSize</item>
        <item name="android:layout_marginTop">0.3dp</item>
        <item name="android:layout_marginRight">0.3dp</item>
    </style>
    <style name="middleNumbers" parent="android:Widget.Material.Button.Borderless.Colored">
        <item name="android:textColor">#ff2d2d2d</item>
        <item name="android:background">#ffeaeaea</item>
        <item name="android:textSize">@dimen/fontSize</item>
        <item name="android:layout_marginTop">0.3dp</item>
        <item name="android:layout_marginRight">0.3dp</item>
    </style>
    <style name="rightSymbols" parent="android:Widget.Material.Button.Borderless">
        <item name="android:background">#ff1c90c5</item>
        <item name="android:textSize">@dimen/fontSize</item>
        <item name="android:textColor">#fff</item>
        <item name="android:layout_marginTop">0.3dp</item>
    </style>
</resources>